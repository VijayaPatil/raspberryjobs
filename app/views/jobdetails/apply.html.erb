<h1>Welcome to apply page</h1>

<%= @form %>

<%= form_tag("../applications/show", :method => "post") do %>
    <%= hidden_field_tag 'jobdid', @jobDID  %>
    <br>
    <%= label_tag(:fn, "First Name:") %>
    <%= text_field_tag(:fn) %>
    <br>
    <%= label_tag(:ln, "Last Name:") %>
    <%= text_field_tag(:ln) %>
    <br>
    <%= submit_tag("Apply") %>
<% end %>