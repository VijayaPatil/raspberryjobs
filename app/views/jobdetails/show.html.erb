 <% @job = @jobdetail.ResponseJob.Job %>

<h1><%= @job.JobTitle %></h1>
<br><br>

 <% if @job.ExternalApplication == 'True' %>
     <%= link_to "External Apply", 'https://www.careerbuilder.com/ApplyOnline/ExternalApply.aspx?useframes=True&Job_DID='.to_s() + @job.DID %>
 <% else %>
     <%= link_to "Apply Here", jobdetailapply_path(:jobDID => @job.DID) %>
 <% end %>
 <br><br>
 <%= @job.inspect %>
 <br><br>






